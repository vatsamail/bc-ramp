pragma solidity ^0.6.0;

import "./lesson_one.sol";

// factory pattern
contract StorageFactory {
    SimpleStore[] public simp_store_array;
    function createSimpleStorageContract() public {
        SimpleStore simp_store = new SimpleStore();
        simp_store_array.push(simp_store);
    }
}